    "license": "license",
    "authors": [
        {
            "name": "Marko Kodric",
            "email": "marko.kodric9@gmail.com"
        }
    ],
    "homepage": "https://github.com/mare06xa/beez",
    "keywords": ["Laravel", "Beez"],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/markoKodric/guzzle"
        }
    ],
    "require": {
        "illuminate/support": "~5",
        "guzzlehttp/guzzle": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Mare06xa\\Beez\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mare06xa\\Beez\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mare06xa\\Beez\\BeezServiceProvider"
            ],
            "aliases": {
                "Beez": "Mare06xa\\Beez\\Facades\\Beez"
            }
        }
    }
}
